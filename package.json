{
  "name": "aws-param-store",
  "version": "4.0.0-Beta1",
  "description": "Library for manipulating parameters in the AWS SSM Parameter Store",
  "keywords": [
    "AWS",
    "SSM",
    "secret",
    "parameter store",
    "parameter",
    "secure",
    "secrets",
    "store"
  ],
  "main": "lib/index.js",
  "scripts": {
    "test": "nyc mocha './lib/**/__tests__/*' --recursive",
    "lint": "eslint lib/*"
  },
  "nyc": {
    "all": true,
    "check-coverage": true,
    "include": [
      "lib/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vandium-io/aws-param-store.git"
  },
  "files": [
    "/lib"
  ],
  "engines": {
    "node": ">=12"
  },
  "author": "Vandium Software Inc.",
  "license": "BSD-3-Clause",
  "dependencies": {
    "sync-rpc": "^1.3.6"
  },
  "peerDependencies": {
    "aws-sdk": "*"
  },
  "devDependencies": {
    "aws-sdk": "^2.610.0",
    "chai": "^4.1.2",
    "eslint": "^6.8.0",
    "eslint-plugin-mocha": "^6.2.2",
    "mocha": "^7.0.1",
    "nyc": "^15.0.0",
    "rewiremock": "^3.13.9",
    "sinon": "^8.1.1"
  }
}
